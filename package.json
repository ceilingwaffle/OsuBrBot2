  "scripts": {
    "start": "rmdir /S /Q dist & tsc & xcopy .\\src\\locales .\\dist\\locales\\ /E /C /Q /Y & node dist/index.js",
    "start:ts": "rmdir /S /Q dist & tsc & ts-node src/index.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "entity:create": "ts-node ./node_modules/typeorm/cli.js entity:create -n",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed --config ormconfig.json",
    "delgo": "rmdir /S /Q dist & tsc & del .\\src\\database\\app-database.sqlite & npm run seed & npm start",
    "test": "rmdir /S /Q dist & tsc & xcopy .\\src\\locales .\\dist\\locales\\ /E /C /Q /Y & mocha --require ts-node/register test/**/*.ts --timeout 30000",
    "build": "rmdir /S /Q dist & tsc",
    "cli:command": "ts-node ./cli/command.ts"
  },
